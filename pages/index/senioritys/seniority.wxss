/* 排行榜 */
.seniority .seniority_header{
  width: 100%;
  height: 80rpx;
  position: relative;
  padding-top: 20rpx;
  font-size: 18px;
  font-weight: bold;
}
.seniority .seniority_header text{
  float: left;
  padding-left: 10rpx;
}
.seniority_content{
  display:flex;
  justify-content:space-between;
  flex-wrap:wrap;
}
.seniority_content .seniority_list{
  width:calc(100% - 8px);
  margin-bottom: 10rpx;
  overflow: hidden;
  height: 262rpx;
  padding-left: 10rpx;
}
.seniority_content .seniority_list .main_left{
  float: left;
  width: 30%;
  position: relative;
  font-size: 12px;
  color: #fff;
}
.seniority_content .seniority_list .main_left image{
  display:inline-block;
  height:100px;
  vertical-align:middle;
  width:100px;
  border-radius: 10rpx;
}
.seniority_content .seniority_list .main_left .upgrape{
  display: block;
  left: 0;
  bottom: 0;
  position: absolute;
}
.seniority_content .seniority_list .main_right{
  float: right;
  width: 66%;
  height: 261rpx;
  border-bottom: 1px solid #f0f0f0;
  padding-top: 6px;
}
.seniority_content .seniority_list .main_right text{
  display: block;
  font-size: 15px;
  color:#323436;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  line-height: 32px;
}
